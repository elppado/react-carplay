{
  "name": "CarPlay",
  "version": "1.0.2",
  "description": "CarPlay",
  "main": "out/main/index.js",
  "scripts": {
    "start": "npx electron-vite preview",
    "dev": "npx electron-vite dev",
    "build": "npx electron-vite build",
    "build:mac": "npx electron-vite build && npx electron-builder --mac --arm64 --config --publish never",
    "build:linux": "npx electron-vite build && npx electron-builder --linux --config --publish never",
    "build:armLinux": "npx electron-vite build && npx electron-builder --linux --armv7l --config --publish never",
    "postinstall": "electron-builder install-app-deps",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@mui/material": "^5.15.10",
    "electron-store": "^8.1.0",
    "events": "^3.3.0",
    "node-carplay": "github:rhysmorgan134/node-CarPlay",
    "pcm-ringbuf-player": "github:rhysmorgan134/pcm-ringbuf-player",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.22.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "electron": "^28.2.3",
    "electron-builder": "^24.9.1",
    "electron-vite": "^3.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^6.3.5"
  }
}
